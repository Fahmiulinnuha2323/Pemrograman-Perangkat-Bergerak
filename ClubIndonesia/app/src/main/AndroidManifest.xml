<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.funqud.clubindonesia">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bola1"
        android:roundIcon="@mipmap/bola1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DetailListClub" android:label="@string/appdetail"></activity>
        <activity android:name=".profile" android:label="@string/app_profile" />
        <activity android:name=".MainActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>